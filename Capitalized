def solve(s):
    t=s.split()
    t1=(i.capitalize() for i in t)
    return ' '.join(t1)
if __name__ == '__main__':
    fptr = open(os.environ['OUTPUT_PATH'], 'w')

    s = input()

    result = solve(s)

    fptr.write(result + '\n')

    fptr.close()

